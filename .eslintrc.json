{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-alert": "error",
    "require-await": "error",
    "no-var": "error"
  },
  "env": {
    "commonjs": true,
    "browser": true,
    "node": true,
    "es6": true
  }
}